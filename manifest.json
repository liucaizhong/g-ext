{
    "manifest_version": 2,
    "name": "SAP Uberbot",
    "version": "1.0",
    "description": "SAP Uberbot for Google Extension",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "author": "Caizhong Liu",
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": ["https://*.sap.corp/*"],
            "run_at": "document_start",
            "js": ["Hello.js"]
        }
    ]
}